{
  "entrypoint": "index.html",
  "shell": "src/nanco-app.html",
  "fragments": [
    "src/nanco-view-home.html",
    "src/nanco-view-portfolio.html",
    "src/nanco-view-contact.html",
    "src/nanco-view-error404.html"
  ],
  "sources": [
    "src/**/*",
    "php/**/*",
    "images/**/*"
  ],
  "extraDependencies": [
    ".htaccess",
    "manifest.json",
    "bower_components/webcomponentsjs/*.js"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
  "builds": [
    {
      "preset": "es5-bundled"
    },
    {
      "preset": "es6-bundled"
    },
    {
      "preset": "es6-unbundled"
    }
  ]
}
